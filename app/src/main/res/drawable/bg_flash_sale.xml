<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--设置为false为显示状态-->
    <item android:state_pressed="false">
        <shape>
            <!-- 设置圆角矩形 -->
            <corners android:radius="1dp" />
            <!-- 文本框里面的颜色 -->
            <solid android:color="@color/default_gray"/>
        </shape>
    </item>
</selector>